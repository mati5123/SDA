from random import randint

los = randint(1, 10)
odp = -1
i = 0
print("zgadnij liczbę z przedziału 1-10")

while odp != los:
     i += 1
     odp = int(input("Podaj liczbę: "))
     if odp > los:
         print("sproboj ponownie jest mniejsza")
     elif odp < los:
         print("wiksz liczba niestety")
print("brawko", i , "razem")
